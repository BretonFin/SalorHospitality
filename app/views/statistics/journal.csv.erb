<% header = "date;user;table;order#;article name;quantity name;price;count;cost_center" -%>
<%= header %>
<% @orders.each do |o| -%>
<% o.items.each do |i| -%>
<% line = sprintf('%s;%s;%s;%s;%s;%s;%.2f;%i;%s', i.order.created_at, i.order.user.login, i.order.table.name, i.order.id, i.article.name, i.quantity ? i.quantity.name : '-', i.quantity ? i.quantity.price : i.article.price, i.count, i.order.cost_center.name) -%>
<%= line %>
<% end %>
<% end %>
