page << '$("invoiceform").innerHTML = "";'

@last_orders.each do |o|
  page.insert_html :bottom, 'invoiceform', render(:partial => 'invoice_form', :locals => { :invoice_form => o, :cost_centers => @cost_centers })
end

page.show 'invoiceform'
page.hide 'orderform'
page.hide 'tables'
page.hide 'rooms'

page << "tableupdates = false;"
