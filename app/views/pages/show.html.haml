%h2= t('activerecord.models.page.other')

= link_to (t :new), new_page_path, :class => 'links'
= link_to (t :edit), edit_page_path(@page), :class => 'links'

.page
  .previous_page_button= link_to '<', page_path(@previous_page) unless @previous_page == @page
  #page= render :partial => 'page', :locals => { :page => @page, :partial_htmls => @partial_htmls }
  .next_page_button= link_to '>', page_path(@next_page) unless @next_page == @page
